Header
  CHECK KEYWORDS Warn
  Mesh DB "." "."
  Include Path ""
  Results Directory "./results"
End

Simulation
  Max Output Level = 10
  Coordinate System = Cartesian
  Coordinate Mapping(3) = 1 2 3
  Simulation Type = Transient
  Output Intervals(1) = 1
  Timestep intervals(1) = 1000
  Timestep Sizes(1) = 1e-3
  Timestepping Method = BDF
  BDF Order = 1
  Solver Input File = case.sif
  Post File = Elmer_.vtu
!  Output File = output.dat
End

Constants
  Permittivity of Vacuum = 8.85418781e-12
  Permeability of Vacuum = 1.25663706e-6
End

Body 1
  Target Bodies(1) = 1
  Name = "Body Property 1"
  Equation = 1
  Material = 1
  Body Force = 1
  Initial condition = 1
End

Solver 1
  Equation = Result Output
  Scalar Field 1 = Temperature
  Scalar Field 2 = Potential
  Scalar Field 3 = Joule Heating
  Vector Field 1 = Current Density
  Vector Field 2 = Magnetic Field Strength
  Vector Field 3 = Magnetic Flux Density
  Output File Name = Heat_Magnetostatics
  Output Format = Vtu
  Procedure = "ResultOutputSolve" "ResultOutputSolver"
  Exec Solver = After Timestep
End

Solver 2
  Equation = MgDynPost
  Procedure = "MagnetoDynamics" "MagnetoDynamicsCalcFields"
  Exec Solver = Before Timestep
  Calculate Joule Heating = Logical True
  Calculate Magnetic Flux Density = Logical True
  Calculate Magnetic Field Strength = Logical True
  Stabilize = True
  Optimize Bandwidth = True
  Nonlinear System Relaxation Factor = 1
  Linear System Solver = Iterative
  BiCGstabl polynomial degree = 2
  Linear System Preconditioning = ILU0
  Linear System Residual Output = 10
End

Solver 3
  Equation = MgDyn
  Procedure = "MagnetoDynamics" "WhitneyAVSolver"
  Exec Solver = Before Timestep
  Calculate Current Density = Logical True
  Optimize Bandwidth = True
  Nonlinear System Max Iterations = 20
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-3
  Nonlinear System Relaxation Factor = 1
  Linear System Solver = Iterative
  Linear System Preconditioning = ILU0
  Linear System Residual Output = 10
End

Solver 4
  Equation = Heat Equation
  Variable = Temperature
  Procedure = "HeatSolve" "HeatSolver"
  Exec Solver = Always
  Stabilize = True
  Optimize Bandwidth = True
  Nonlinear System Convergence Tolerance = 1.0e-7
  Nonlinear System Max Iterations = 20
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-3
  Nonlinear System Relaxation Factor = 1
  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 500
  Linear System Convergence Tolerance = 1.0e-10
  Linear System Preconditioning = ILU0
  Linear System Residual Output = 10
  Linear System Precondition Recompute = 1
End

Solver 5
  Equation = Stat Current Solver
  Procedure = "StatCurrentSolve" "StatCurrentSolver"
  Variable = Potential
  Calculate Joule Heating = Logical True
  Calculate Volume Current = Logical True
  Calculate Nodal Heating = False
  Calculate Nodal Potential = Logical True
  Exec Solver = Before Timestep
  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 500
  Linear System Convergence Tolerance = 1.0e-10
  Linear System Preconditioning = ILU0
  Linear System Residual Output = 10
  Linear System Precondition Recompute = 1
End

Equation 1
  Name = "Electrostatics_HeatEquation_MagnetoDynamics"
  Active Solvers(5) = 5 3 2 4 1
End

Material 1
  Name = "Copper (generic)"
  Emissivity = 0.04
  Youngs modulus = 115.0e9
  Heat Capacity = 385.0
  Heat expansion Coefficient = 16.5e-6
  Poisson ratio = 0.34
  Heat Conductivity = 401.0
  Sound speed = 3810.0
  Density = 8960.0
  Relative Permeability = 0.999994
  Relative Permittivity = 1
  Electric Conductivity = 59.59e6
End

Body Force 1
  Name = "BodyForce 1"
  Joule Heat = Logical True
  Heat Source = Equals Joule Heating
End

Initial Condition 1
  Name = "InitialCondition 1"
  Potential = 0
  AV {e} = 0
  AV = 0
  Temperature = 293.15
End

Boundary Condition 1
  Target Boundaries(1) = 1 
  Name = "Boundary 1"
  External Temperature = 293.15
  AV = 0
  Heat Transfer Coefficient = 10
  Potential = 24
End

Boundary Condition 2
  Target Boundaries(1) = 2 
  Name = "Boundary 2"
  Heat Transfer Coefficient = 10
  AV = 0
  External Temperature = 293.15
End

Boundary Condition 3
  Target Boundaries(1) = 3 
  Name = "Boundary 3"
  Heat Transfer Coefficient = 10
  AV = 0
  External Temperature = 293.15
  Potential = 0
End

Boundary Condition 4
  Target Boundaries(1) = 4 
  Name = "Boundary 4"
  External Temperature = 293.15
  Heat Transfer Coefficient = 10
  AV = 0
End

Boundary Condition 5
  Target Boundaries(1) = 5 
  Name = "Boundary 5"
  Heat Transfer Coefficient = 10
  AV = 0
  External Temperature = 293.15
End

Boundary Condition 6
  Target Boundaries(1) = 6 
  Name = "Boundary 6"
  External Temperature = 293.15
  AV = 0
  Heat Transfer Coefficient = 10
End
